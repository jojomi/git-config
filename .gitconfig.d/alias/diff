[alias]
  # diff
  # ex: git d
  d = diff

  # word diff (can keep small changes oneline)
  # ex: git word-diff
  word-diff = diff --word-diff=color

  # alias: word-diff
  wd = !git wdiff

  # show diff for staged changes (vs. HEAD)
  # ex: git diff-staged
  diff-staged = diff --cached

  # alias: staged
  ds = !git staged
